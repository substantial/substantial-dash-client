<h2>Upcoming</h2>

{{#each contents}}
  {{#view view.hotnessView time=starts_at}}
    <div class="feed-event">
      <p>{{summary}}</p>
      <p class="details">
        {{time-ago-in-words starts_at view.clock.pulse}}
        {{#if location}}at {{location}}{{/if}}</p>
    </div>
  {{/view}}
{{else}}
  <p>None</p>
{{/each}}
