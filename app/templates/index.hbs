{{#if auth}}

<p class="details">Authenticated as {{auth.name}}</p>
  
<div class="flex-grid">
  <div class="flex-cell">
    {{github-feed title="Code" channel="github-org-feed-substantial-sf"}}
  </div>
  <div class="flex-cell">
    {{upcoming-events title="Upcoming" channel="icalendar-upcoming"}}
  </div>
</div>

{{else}}

